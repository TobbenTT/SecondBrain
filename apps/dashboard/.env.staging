# Server
PORT=3010
NODE_ENV=staging

# PostgreSQL (local en staging)
PG_HOST=postgres-staging
PG_PORT=5432
PG_USER=secondbrain
PG_PASSWORD=staging_sb_2026
PG_DATABASE=secondbrain

